<%- include('../includes/head.ejs') %>
<%- include('../includes/input-validation/edit-partner.ejs') %>

</head>

<body>
    <div class="dashboard-main-wrapper">
      <%- include('../includes/navigation.ejs') %>
      <%- include('../includes/menu.ejs') %>
      <form 
         id="edit-partner"
         class="ms-content-wrapper" 
         action="/super-admin/<% if (editing) { %>edit-partner<% } %>"
         method="POST"
         enctype="multipart/form-data">

         <% if (editing) { %>
            <input
               type="hidden"
               value="<%= partnerId %>"
               name="partnerId" />
        <% } %>
              
               <div class="dashboard-wrapper">
                <div class="container-fluid dashboard-content">
                    <div class="row">
                        <div class="col-xl-10">
                            <div class="row">
                                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                    <div class="section-block" id="basicform">
                                        <h3 class="section-title">Partner szerkesztése</h3>
                                    </div>
                                    <div class="card">                      
                                        <div class="card-body">

                                                 <div class="form-group">
                                                   <label for="inputText3" class="col-form-label">Partner <strong>Id</strong></label>
                                                   <input id="id" name="roName" type="text" class="form-control" disabled value="<% if (editing) { %><%= partners[0].id %><% } %>">
                                                 </div>

                                                <div class="form-group">
                                                   <label for="inputText3" class="col-form-label">Partner <strong>név</strong></label>
                                                   <input id="fullName" name="fullName" type="text" class="form-control" value="<% if (editing) { %><%= partners[0].fullName %><% } %>">
                                                   <span class="check-input error_form intro" id="fullName_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                   <label for="inputText3" class="col-form-label">Partner <strong>telefonszáma</strong></label>
                                                   <input id="phoneNumber" name="phoneNumber" type="text" class="form-control" value="<% if (editing) { %><%= partners[0].phoneNumber %><% } %>">
                                                   <span class="check-input error_form intro" id="phoneNumber_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                   <label for="inputText3" class="col-form-label">Partner <strong>e-mail címe</strong></label>
                                                   <input id="email" name="email" type="text" class="form-control" value="<% if (editing) { %><%= partners[0].email %><% } %>">
                                                   <span class="check-input error_form intro" id="email_error_message"></span>
                                                </div>   
                                                
                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner <strong>jutaléka</strong></label>
                                                    <input id="commission" name="commission" type="text" class="form-control" value="<% if (editing) { %><%= partners[0].commission %><% } %>">
                                                    <span class="check-input error_form intro" id="commission_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner <strong>nyitási idő</strong></label>
                                                    <input id="open" name="open" type="text" class="form-control" value="<% if (editing) { %><%= partners[0].open %><% } %>">
                                                    <span class="check-input error_form intro" id="open_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner <strong>zárási idő</strong></label>
                                                    <input id="close" name="close" type="text" class="form-control" value="<% if (editing) { %><%= partners[0].close %><% } %>">
                                                    <span class="check-input error_form intro" id="close_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner rövid <strong>román</strong> bemutatkozása</label>
                                                    <textarea
                                                        id="partnerShortDescRoView"
                                                        name="partnerShortDescRoView"
                                                        type="text"
                                                        class="form-control">
                                                        <% if (editing || hasError) { %><%= partnerShortDescRoView %><% } %></textarea>
                                                        <span class="check-input error_form intro" id="partnerShortDescRoView_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner rövid <strong>magyar</strong> bemutatkozása</label>
                                                    <textarea                            
                                                        name="partnerShortDescHuView"
                                                        type="text"
                                                        id="partnerShortDescHuView"
                                                        class="form-control">
                                                        <% if (editing || hasError) { %><%= partnerShortDescHuView %><% } %></textarea>
                                                        <span class="check-input error_form intro" id="partnerShortDescHuView_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner rövid <strong>angol</strong> bemutatkozása</label>
                                                    <textarea                                                        
                                                        name="partnerShortDescEnView"
                                                        type="text"
                                                        id="partnerShortDescEnView"
                                                        class="form-control test">
                                                        <% if (editing || hasError) { %><%= partnerShortDescEnView %><% } %></textarea>
                                                        <span class="check-input error_form intro" id="partnerShortDescEnView_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner <strong>román</strong> címe</label>
                                                    <input id="addressRo" name="addressRo" type="text" class="form-control" value="<% if (editing) { %><%= partnerAdressRoView %><% } %>">
                                                    <span class="check-input error_form intro" id="addressRo_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner <strong>magyar</strong> címe</label>
                                                    <input id="addressHu" name="addressHu" type="text" class="form-control" value="<% if (editing) { %><%= partnerAdressHuView %><% } %>">
                                                    <span class="check-input error_form intro" id="addressHu_error_message"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputText3" class="col-form-label">Partner <strong>angol</strong> címe</label>
                                                    <input id="addressEn" name="addressEn" type="text" class="form-control" value="<% if (editing) { %><%= partnerAdressEnView %><% } %>">
                                                    <span class="check-input error_form intro" id="addressEn_error_message"></span>
                                                </div>
                                        </div>
                                    </div>
                                    <button type="submit" id="partner-btn" class="btn btn-success has-icon" ><% if (editing) { %>Partner adatok frissítése<% } %></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          </div>
        </form>
    </body>
</html>

<%- include('../includes/end.ejs') %>


