<%# Partner validation %>
<script>
  $(function () {
    $("#fullName_error_message").hide();
    $("#phoneNumber_error_message").hide();
    $("#email_error_message").hide();
    $("#commission_error_message").hide();
    $("#open_error_message").hide();
    $("#close_error_message").hide();
    $("#partnerShortDescRoView_error_message").hide();
    $("#partnerShortDescHuView_error_message").hide();
    $("#partnerShortDescEnView_error_message").hide();
    $("#addressRo_error_message").hide();
    $("#addressHu_error_message").hide();
    $("#addressEn_error_message").hide();

    var fullName = false;
    var phoneNumber = false;
    var email = false;
    var commission = false;
    var open = false;
    var close = false;
    var partnerShortDescRoView = false;
    var partnerShortDescHuView = false;
    var partnerShortDescEnView = false;
    var addressRo = false;
    var addressHu = false;
    var addressEn = false;

    $("#fullName").focusout(function () {
      check_fullName();
    });

    $("#phoneNumber").focusout(function () {
      check_phoneNumber();
    });

    $("#email").focusout(function () {
      check_email();
    });

    $("#commission").focusout(function () {
      check_commission();
    });

    $("#open").focusout(function () {
      check_open();
    });

    $("#close").focusout(function () {
      check_close();
    });

    $("#partnerShortDescRoView").focusout(function () {
      check_partnerShortDescRoView();
    });

    $("#partnerShortDescHuView").focusout(function () {
      check_partnerShortDescHuView();
    });

    $("#partnerShortDescEnView").focusout(function () {
      check_partnerShortDescEnView();
    });

    $("#addressRo").focusout(function () {
      check_addressRo();
    });

    $("#addressHu").focusout(function () {
      check_addressHu();
    });

    $("#addressEn").focusout(function () {
      check_addressEn();
    });

    function check_fullName() {
      var fullName_length = $("#fullName").val().length;

      if (fullName_length < 1 || fullName_length > 25) {
        $("#fullName_error_message").html(
          "Teljes név 1 és 25 karakter között kell lennie"
        );
        $("#fullName_error_message").show();
        fullName = true;
      } else {
        $("#fullName_error_message").hide();
      }
    }

    function check_phoneNumber() {
      var phoneNumber_length = $("#phoneNumber").val().length;

      if (phoneNumber_length < 4 || phoneNumber_length > 25) {
        $("#phoneNumber_error_message").html(
          "Telefonszámnak 4 és 25 karakter között kell lennie"
        );
        $("#phoneNumber_error_message").show();
        phoneNumber = true;
      } else {
        $("#phoneNumber_error_message").hide();
      }
    }

    function check_email() {
      var email_length = $("#email").val().length;

      if (email_length < 2 || email_length > 20) {
        $("#email_error_message").html(
          "Extra neve 2 és 25 karakter között kell lennie"
        );
        $("#email_error_message").show();
        email = true;
      } else {
        $("#email_error_message").hide();
      }
    }

    function check_commission() {
      var commission_length = $("#commission").val().length;

      if (commission_length < 1 || commission_length > 5) {
        $("#commission_error_message").html(
          "Komisszió 1 és 5 karakter között kell lennie"
        );
        $("#commission_error_message").show();
        commission = true;
      } else {
        $("#commission_error_message").hide();
      }
    }

    function check_open() {
      var open_length = $("#open").val().length;

      if (open_length < 2 || open_length > 20) {
        $("#open_error_message").html(
          "Nyitási idő 2 és 25 karakter között kell lennie"
        );
        $("#open_error_message").show();
        open = true;
      } else {
        $("#open_error_message").hide();
      }
    }

    function check_close() {
      var close_length = $("#close").val().length;

      if (close_length < 2 || close_length > 25) {
        $("#close_error_message").html(
          "Zárási idő 2 és 25 karakter között kell lennie"
        );
        $("#close_error_message").show();
        close = true;
      } else {
        $("#close_error_message").hide();
      }
    }

    function check_partnerShortDescRoView() {
      var partnerShortDescRoView_length = $("#partnerShortDescRoView").val()
        .length;

      if (
        partnerShortDescRoView_length < 10 ||
        partnerShortDescRoView_length > 400
      ) {
        $("#partnerShortDescRoView_error_message").html(
          "Román bemutatkozás 10 és 300 karakter között kell lennie"
        );
        $("#partnerShortDescRoView_error_message").show();
        partnerShortDescRoView = true;
      } else {
        $("#partnerShortDescRoView_error_message").hide();
      }
    }

    function check_partnerShortDescHuView() {
      var partnerShortDescHuView_length = $("#partnerShortDescHuView").val()
        .length;

      if (
        partnerShortDescHuView_length < 2 ||
        partnerShortDescHuView_length > 400
      ) {
        $("#partnerShortDescHuView_error_message").html(
          "Magyar bemutatkozás 10 és 400 karakter között kell lennie"
        );
        $("#partnerShortDescHuView_error_message").show();
        partnerShortDescHuView = true;
      } else {
        $("#partnerShortDescHuView_error_message").hide();
      }
    }

    function check_partnerShortDescEnView() {
      var partnerShortDescEnView_length = $("#partnerShortDescEnView").val()
        .length;

      if (
        partnerShortDescEnView_length < 2 ||
        partnerShortDescEnView_length > 400
      ) {
        $("#partnerShortDescEnView_error_message").html(
          "Angol bemutatkozás 10 és 400 karakter között kell lennie"
        );
        $("#partnerShortDescEnView_error_message").show();
        partnerShortDescEnView = true;
      } else {
        $("#partnerShortDescEnView_error_message").hide();
      }
    }

    function check_addressRo() {
      var addressRo_length = $("#addressRo").val().length;

      if (addressRo_length < 2 || addressRo_length > 500) {
        $("#addressRo_error_message").html(
          "Román címnek 2 és 500 karakter között kell lennie"
        );
        $("#addressRo_error_message").show();
        addressRo = true;
      } else {
        $("#addressRo_error_message").hide();
      }
    }

    function check_addressHu() {
      var addressHu_length = $("#addressHu").val().length;

      if (addressHu_length < 2 || addressHu_length > 500) {
        $("#addressHu_error_message").html(
          "Magyar címnek 2 és 500 karakter között kell lennie"
        );
        $("#addressHu_error_message").show();
        addressHu = true;
      } else {
        $("#addressHu_error_message").hide();
      }
    }

    function check_addressEn() {
      var addressEn_length = $("#addressEn").val().length;

      if (addressEn_length < 2 || addressEn_length > 500) {
        $("#addressEn_error_message").html(
          "Angol címnek 2 és 500 karakter között kell lennie"
        );
        $("#addressEn_error_message").show();
        addressEn = true;
      } else {
        $("#addressEn_error_message").hide();
      }
    }

    $("#edit-partner").submit(function () {
      fullName = false;
      phoneNumber = false;
      email = false;
      commission = false;
      open = false;
      close = false;
      partnerShortDescRoView = false;
      partnerShortDescHuView = false;
      partnerShortDescEnView = false;
      addressRo = false;
      addressHu = false;
      addressEn = false;

      check_fullName();
      check_phoneNumber();
      check_email();
      check_commission();
      check_open();
      check_close();

      check_partnerShortDescRoView();
      check_partnerShortDescHuView();
      check_partnerShortDescEnView();

      check_addressRo();
      check_addressHu();
      check_addressEn();

      if (
        fullName == false &&
        phoneNumber == false &&
        email == false &&
        commission == false &&
        open == false &&
        close == false &&
        partnerShortDescRoView == false &&
        partnerShortDescHuView == false &&
        partnerShortDescEnView == false &&
        addressRo == false &&
        addressHu == false &&
        addressEn == false
      ) {
        return true;
      } else {
        return false;
      }
    });
  });
</script>

<script>
  $(document).ready(function () {
    $("#edit-partner").submit(function () {
      $(".partner-btn").attr("disabled", true);
      return true;
    });
  });
</script>

<%# End Partner validation %>
