<thead>
  <tr>
    <th scope="col">ID</th>
    <th scope="col">Name</th>

    <th scope="col">STATUS</th>
    <th scope="col">EDIT</th>
  </tr>
</thead>
<% if (box.length > 0) { %> <% for (let i = 0; i < box.length; i++) { %>
<tbody>
  <th scope="row"><%= box[i].id %></th>
  <td><%= box[i].BoxTranslations[0].name %></td>

  <td><span class="badge badge-success">AKTIV</span></td>

  <td>
    <a
      href="/admin/edit-box/<%= box[i].id %>?edit=true"
      class="btn grid-btn mt-0 btn-sm btn-secondary"
      ><%= lingua.product_action_edit %></a
    >
  </td>
</tbody>
<% } %> <% } else { %>
<h1>No Products Found!</h1>
<% } %>
