<% if (category.length > 0) { %> <% for (let i=0; i < category.length; i++ ) {
%>
<tbody>
  <tr>
    <th scope="row"><%= category[i].id %></th>

    <td>
      <%= category[i].productCategoryTranslations[0].name %>
    </td>
    <td><span class="badge badge-success">AKTIV</span></td>
    <td>
      <a
        href="/admin/edit-category/<%= category[i].id %>?edit=true"
        class="btn grid-btn mt-0 btn-sm btn-secondary"
        ><%= lingua.product_action_edit %></a
      >
    </td>
  </tr>
</tbody>
<% } %> <% } else { %>
<h1>No Products Found!</h1>
<% } %>
