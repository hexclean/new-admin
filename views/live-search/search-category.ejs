<thead>
  <tr>
    <th>ID</th>
    <th>KATEGÓRIA NÉV</th>
    <th>MÜVELET</th>
  </tr>
</thead>
<% for (let i=0; i < category.length; i++ ) { %>
<tbody>
  <tr>
    <td><%= category[i].id %></td>
    <td><%= category[i].CategoryTranslations[0].name %></td>
    <td>
      <a
        class="btn btn-secondary btn-sm"
        href="/admin/edit-category/<%= category[i].id %>?edit=true"
      >
        <i class="fas fa-pencil-alt"> </i>
        <%= lingua.button_edit %>
      </a>
    </td>
  </tr>
</tbody>
<% } %>
