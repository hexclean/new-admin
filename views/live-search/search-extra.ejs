<% if (extra.length > 0) { %> <% for (let i=0; i < extra.length; i++ ) { %>
<tbody>
  <tr>
    <th scope="row"><%= extra[i].id %></th>

    <td><%= extra[i].extraTranslations[0].name %></td>
    <td><span class="badge badge-success">AKTIV</span></td>
    <td>
      <a
        href="/admin/edit-extra/<%= extra[i].id %>?edit=true"
        class="btn grid-btn mt-0 btn-sm btn-secondary"
        ><%= lingua.product_action_edit %></a
      >
    </td>
  </tr>
</tbody>
<% } %> <% } else { %>
<h1>No Products Found!</h1>
<% } %>
