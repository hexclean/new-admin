      <script>
         const TOTAL_COLUMN = 5;
         $(document).on("click", ".move-order-next", function () {
           var id = $(this).attr("data-id");
           var order = $("#" + id);
           var currentCol = parseInt($(order).attr("data-current-column"));
           var nextCol = parseInt(currentCol) + parseInt(1);
         
           if (nextCol > TOTAL_COLUMN) {
             nextCol = TOTAL_COLUMN;
           }
         
           if (nextCol === 2) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/next",
               type: "POST",
               data: {
                 orderStatusId: 2,
                 orderId: id,
               },
               
             });
             
           }
         
           if (nextCol === 3) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/next",
               type: "POST",
               data: {
                 orderStatusId: 3,
                 orderId: id,
               },
               
             });
             
           }
         
           if (nextCol === 4) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/next",
               type: "POST",
               data: {
                 orderStatusId: 4,
                 orderId: id,
               },
               
             });
             
           }
         
           if (nextCol === 5) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/next",
               type: "POST",
               data: {
                 orderStatusId: 5,
                 orderId: id,
               },
               
             });
             
           }
         
           $(order).attr("data-current-column", nextCol);
           $(order).prependTo("#column-" + nextCol);
         });
         
         $(document).on("click", ".move-order-prev", function () {
           var id = $(this).attr("data-id");
           var order = $("#" + id);
           var currentCol = parseInt($(order).attr("data-current-column"));
           var nextCol = parseInt(currentCol) - parseInt(1);
         
           if (nextCol < 1) {
             nextCol = 1;
           }
         
           if (nextCol === 2) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/prev",
               type: "POST",
               data: {
                 orderStatusId: 2,
                 orderId: id,
               },
               
             });
             
           }
         
           if (nextCol === 3) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/prev",
               type: "POST",
               data: {
                 orderStatusId: 3,
                 orderId: id,
               },
               
             });
             
           }
         
           if (nextCol === 4) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/prev",
               type: "POST",
               data: {
                 orderStatusId: 4,
                 orderId: id,
               },
               
             });
             
           }
         
           if (nextCol === 5) {
             $.ajax({
               url: "http://admin.foodnet.ro/admin/ajax/prev",
               type: "POST",
               data: {
                 orderStatusId: 5,
                 orderId: id,
               },
               
             });
             
           }
         
           $(order).attr("data-current-column", nextCol);
           $(order).prependTo("#column-" + nextCol);
         });
      </script>
